<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/newsportal/settings/wcm/templates/news-template"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="newsportal/components/structure/page">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="newsportal/components/structure/navigation"/>
        <text_on_hero_image
                jcr:primaryType="nt:unstructured"
                sling:resourceType="newsportal/components/content/text-on-hero-image"/>
        <contentfragment
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dam/cfm/components/contentfragment"
                text=""/>
        <comments_and_likes_379776137
                jcr:primaryType="nt:unstructured"
                sling:resourceType="newsportal/components/content/comments-and-likes"/>
        <related_news
                jcr:primaryType="nt:unstructured"
                sling:resourceType="newsportal/components/content/related-news"/>
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/responsivegrid"/>
        <footer
            jcr:lastModifiedBy="admin"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="newsportal/components/structure/footer"/>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <phone
                    jcr:primaryType="nt:unstructured"
                    title="Smaller Screen"
                    width="{Long}650"/>
                <tablet
                    jcr:primaryType="nt:unstructured"
                    title="Tablet"
                    width="{Long}1200"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
